@model IEnumerable<string>


<div class="row">
    <div class="col-md-12 col-sm-12 col-lg-12 col-sx-12 title">#Теги</div>
</div>
<ul>
@if(Model != null)
{
    foreach (var item in Model)
    {
        
        if (!String.IsNullOrEmpty(item))
        {
            <li>
                @Ajax.ActionLink(item ,"Result", "Search", new { search = "#" + item }, new AjaxOptions{ UpdateTargetId = "content" })
           </li>
        }
        
    }

}
else
{
    
}
    </ul>

@System.Web.Optimization.Scripts.Render("~/bundles/my")